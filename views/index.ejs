<%- include("Partials/header")%>

 <h1><%= title %></h1>



<h2>View A Music Collection!</h2>

<a href="views/create-collection"></a>

<ul> <% // creating an unordered list which goes though everything in the createCollection array  %>
  <% createCollection.forEach(p => { %>
    <li>
      <a href="/music-collection/<%= p._id %>"><%= p.title %></a> <% //creates a link for the individual music collections %>

      <form method="POST" action="/music-collection/<%= p._id %>/delete" style="display:inline"> <% //creating the delete form for %>
        <button>Delete</button>
      </form>
    </li>
  <% }) %>
</ul>

<%- include("Partials/footer")%>